        "import numpy as np\n",
        "from scipy.io import loadmat\n",
        "import deepxde as dde\n",
        "import paddle\n",
        "\n",
        "paddle.set_device('cpu')  
        "\n",
        "DATA_PATH = r\"C:\\Users\\sauja\\Downloads\\cylinder_nektar_wake.mat\"\n",
        "\n",
        "def load_training_data(num_points=5000, noise_level=0.0):\n",
        "    data = loadmat(DATA_PATH)\n",
        "    X_star = data[\"X_star\"]  # (N, 2)\n",
        "    t_star = data[\"t\"]       # (T, 1)\n",
        "    U_star = data[\"U_star\"]  # (N, 2, T)\n",
        "    P_star = data[\"p_star\"]  # (N, T)\n",
        "\n",
        "    N = X_star.shape[0]\n",
        "    T = t_star.shape[0]\n",
        "\n",
        "    XX = np.tile(X_star[:, 0:1], (1, T))  # (N, T)\n",
        "    YY = np.tile(X_star[:, 1:2], (1, T))  # (N, T)\n",
        "    TT = np.tile(t_star.T, (N, 1))        # (N, T)\n",
        "\n",
        "    UU = U_star[:, 0, :]  # (N, T)\n",
        "    VV = U_star[:, 1, :]  # (N, T)\n",
        "    PP = P_star           # (N, T)\n",
        "\n",
        "    x = XX.flatten()[:, None]\n",
        "    y = YY.flatten()[:, None]\n",
        "    t = TT.flatten()[:, None]\n",
        "\n",
        "    u = UU.flatten()[:, None]\n",
        "    v = VV.flatten()[:, None]\n",
        "    p = PP.flatten()[:, None]\n",
        "\n",
        "    data_all = np.hstack((x, y, t, u, v, p))\n",
        "\n",
        "    data_filtered = data_all[\n",
        "        (data_all[:, 2] <= 7) &\n",
        "        (data_all[:, 0] >= 1) &\n",
        "        (data_all[:, 0] <= 8) &\n",
        "        (data_all[:, 1] >= -2) &\n",
        "        (data_all[:, 1] <= 2)\n",
        "    ]\n",
        "\n",
        "    if noise_level > 0:\n",
        "        noise_scale = noise_level / 100.0\n",
        "        noise_u = noise_scale * np.std(data_filtered[:, 3]) * np.random.randn(len(data_filtered))[:, None]\n",
        "        noise_v = noise_scale * np.std(data_filtered[:, 4]) * np.random.randn(len(data_filtered))[:, None]\n",
        "        data_filtered[:, 3] += noise_u.flatten()\n",
        "        data_filtered[:, 4] += noise_v.flatten()\n",
        "\n",
        "    idx = np.random.choice(len(data_filtered), size=num_points, replace=False)\n",
        "\n",
        "    x_train = data_filtered[idx, 0:1]\n",
        "    y_train = data_filtered[idx, 1:2]\n",
        "    t_train = data_filtered[idx, 2:3]\n",
        "    u_train = data_filtered[idx, 3:4]\n",
        "    v_train = data_filtered[idx, 4:5]\n",
        "    p_train = data_filtered[idx, 5:6]\n",
        "\n",
        "    X = np.hstack((x_train, y_train, t_train))\n",
        "    Y = np.hstack((u_train, v_train))\n",
        "\n",
        "    return X, Y\n",
        "\n",
        "def create_model(X, Y):\n",
        "    spatial_domain = dde.geometry.Rectangle([1.0, -2.0], [8.0, 2.0])\n",
        "    time_domain = dde.geometry.TimeDomain(0.0, 7.0)\n",
        "    geomtime = dde.geometry.GeometryXTime(spatial_domain, time_domain)\n",
        "\n",
        "    net = dde.nn.FNN([3] + [50]*3 + [2], \"tanh\", \"Glorot normal\")\n",
        "\n",
        "    C1 = dde.Variable(1.0)\n",
        "    C2 = dde.Variable(0.01)\n",
        "\n",
        "    def pde(x, y):\n",
        "        u = y[:, 0:1]\n",
        "        v = y[:, 1:2]\n",
        "\n",
        "        u_x = dde.grad.jacobian(y, x, i=0, j=0)\n",
        "        u_y = dde.grad.jacobian(y, x, i=0, j=1)\n",
        "        u_t = dde.grad.jacobian(y, x, i=0, j=2)\n",
        "\n",
        "        v_x = dde.grad.jacobian(y, x, i=1, j=0)\n",
        "        v_y = dde.grad.jacobian(y, x, i=1, j=1)\n",
        "        v_t = dde.grad.jacobian(y, x, i=1, j=2)\n",
        "\n",
        "        u_xx = dde.grad.hessian(y, x, component=0, i=0, j=0)\n",
        "        u_yy = dde.grad.hessian(y, x, component=0, i=1, j=1)\n",
        "\n",
        "        v_xx = dde.grad.hessian(y, x, component=1, i=0, j=0)\n",
        "        v_yy = dde.grad.hessian(y, x, component=1, i=1, j=1)\n",
        "\n",
        "        eq1 = u_t + C1 * (u * u_x + v * u_y) - C2 * (u_xx + u_yy)\n",
        "        eq2 = v_t + C1 * (u * v_x + v * v_y) - C2 * (v_xx + v_yy)\n",
        "\n",
        "        return [eq1, eq2]\n",
        "\n",
        "    observe_u = dde.icbc.PointSetBC(X, Y[:, 0:1], component=0)\n",
        "    observe_v = dde.icbc.PointSetBC(X, Y[:, 1:2], component=1)\n",
        "\n",
        "    data = dde.data.PDE(\n",
        "        geomtime,\n",
        "        pde,\n",
        "        [observe_u, observe_v],\n",
        "        num_domain=500,\n",
        "        num_boundary=200,\n",
        "    )\n",
        "\n",
        "    model = dde.Model(data, net)\n",
        "    return model, C1, C2\n",
        "\n",
        "def train_and_predict(noise_level):\n",
        "    X_train, Y_train = load_training_data(num_points=5000, noise_level=noise_level)\n",
        "    model, C1_var, C2_var = create_model(X_train, Y_train)\n",
        "\n",
        "    model.compile(\"adam\", lr=1e-3, external_trainable_variables=[C1_var, C2_var])\n",
        "    model.train(epochs=1000, display_every=200)\n",
        "\n",
        "    return C1_var.numpy(), C2_var.numpy()\n",
        "\n",
        "noise_levels = [0, 1, 5, 10, 20, 50, 60, 75, 90]\n",
        "results = []\n",
        "\n",
        "for nl in noise_levels:\n",
        "    print(f\"\\nTraining for noise level: {nl}%\")\n",
        "    c1_pred, c2_pred = train_and_predict(nl)\n",
        "    results.append([nl, c1_pred, c2_pred])\n",
        "\n",
        "print(\"\\nNoise Level (%) | Predicted C1 | Predicted C2\")\n",
        "print(\"-------------------------------------------\")\n",
        "for nl, c1, c2 in results:\n",
        "    print(f\"{nl:14} | {c1:.5f}      | {c2:.5f}\")\n"
      ]
    }
  ]
